  "license": "MIT",
  "directories": {
    "src": "src",
    "test": "__tests__"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/leapFE/luban.git"
  },
  "bugs": {
    "url": "https://github.com/leapFE/luban/issues"
  },
